<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_faculty_number" model="ir.sequence">
            <field name="name">Faculty Number</field>
            <field name="code">faculty.details</field>
            <field name="prefix">F</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

