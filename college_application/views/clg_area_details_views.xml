<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="clg_area_view" model="ir.ui.view">
      <field name="name">clg.area.form</field>
      <field name="model">clg.area</field>
      <field name="arch" type="xml">
        <form >
            <sheet>
                <h1> Our clg areas</h1>
               <group>
                    <field name="name"/>
                    <field name="image1" widget="image"/>
               </group>
            </sheet>
        </form>
      </field>
    </record>

    <record id="action_clg_area" model="ir.actions.act_window">
        <field name="name">clg.area</field>
        <field name="res_model">clg.area</field>
        <field name="view_mode">form</field>
        <field name="target">inline</field>
    </record>

